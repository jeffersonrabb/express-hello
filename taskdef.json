{
    "family": "express-hello",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "express-hello",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":8080,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
